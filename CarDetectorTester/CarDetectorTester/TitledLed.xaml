<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="CarDetectorTester.TitledLed"
	x:Name="UserControl" d:DesignWidth="215.667" d:DesignHeight="121.75">

	<Grid x:Name="LayoutRoot">
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="2*"/>
		</Grid.RowDefinitions>
		<Label  Grid.Row="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="/CarDetectorTester;component/Fonts/#Digital-7 Mono" FontSize="64" Content="{Binding Speed, ElementName=UserControl}"/>
		<Label HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="21.333" Content="车速"/>
	</Grid>
</UserControl>